{
  "name": "nadro-mentoria-backend",
  "version": "1.0.0",
  "description": "Backend para Sistema de Mentor√≠a Integral - Nadro",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy": "serverless deploy",
    "deploy:qa": "serverless deploy --stage qa",
    "deploy:dev": "serverless deploy --stage dev-jul",
    "deploy:desarrollo": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "remove:qa": "serverless remove --stage dev",
    "remove:dev": "serverless remove --stage dev-jul",
    "remove:desarrollo": "serverless remove --stage prod",
    "info": "serverless info",
    "info:qa": "serverless info --stage dev",
    "info:dev": "serverless info --stage dev-jul",
    "info:desarrollo": "serverless info --stage prod",
    "logs": "serverless logs -f api",
    "logs:qa": "serverless logs -f api --stage dev",
    "logs:dev": "serverless logs -f api --stage dev-jul",
    "logs:desarrollo": "serverless logs -f api --stage prod"
  },
  "keywords": [
    "nadro",
    "mentoria",
    "backend",
    "express",
    "aws"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.916.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "serverless-http": "^4.0.0",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "serverless": "^4.4.7",
    "serverless-offline": "^14.3.3"
  }
}
